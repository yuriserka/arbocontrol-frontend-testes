{
    "plugins": [
        "plugins/markdown",
        "node_modules/better-docs/category"
    ],
    "source": {
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)",
        "include": [
            "./src"
        ]
    },
    "recurseDepth": 10,
    "opts": {
        "destination": "./docs/",
        "template": "node_modules/better-docs/",
        "readme": "./README.md",
        "recurse": true,
        "verbose": true
    },
    "sourceType": "module",
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}