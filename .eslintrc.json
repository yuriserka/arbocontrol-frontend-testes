{
  "extends": ["eslint:recommended", "google"],
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "linebreak-style": ["error", "windows"],
    "max-len": ["error", { "code": 120 }],
    "no-undef": ["warning"]
  }
}
