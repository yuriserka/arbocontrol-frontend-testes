{
  "name": "testes_web",
  "version": "1.0.0",
  "description": "testes desenvolvidos para o sistema Arbocontrol",
  "main": "protractor.conf.ts",
  "scripts": {
    "test": "yarn compile && yarn protractor ./build/configs/protractor.test.conf.js",
    "test:jmx": "yarn compile && yarn protractor ./build/configs/protractor.jmx.conf.js",
    "docs": "yarn typedoc --tsconfig",
    "webdriver:start": "yarn webdriver-manager update --standalone && yarn webdriver-manager start",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc -p .",
    "fix": "gts fix"
  },
  "keywords": [
    "Software Test"
  ],
  "author": "Yuri Serka do Carmo Rodrigues",
  "license": "MIT",
  "dependencies": {
    "cucumber": "^6.0.5",
    "protractor": "^7.0.0",
    "typescript": "^3.7.5"
  },
  "devDependencies": {
    "@types/chai": "^4.2.7",
    "@types/cucumber": "^6.0.1",
    "@types/mkdirp": "^0.5.2",
    "chai": "^4.2.0",
    "cucumber-html-reporter": "^5.1.0",
    "cucumber-pretty": "^6.0.0",
    "dotenv": "^8.2.0",
    "gts": "^1.1.2",
    "mkdirp": "^0.5.1",
    "moment": "^2.25.0",
    "protractor-cucumber-framework": "^6.2.0",
    "protractor-multiple-cucumber-html-reporter-plugin": "^1.8.1",
    "ts-node": "^8.6.2",
    "typedoc": "^0.16.7"
  }
}
